library(ISLR)
fix(College)
summary(College)
pairs(College[,1:10])
plot(Outstate,Private)
attach(College)
plot(Outstate, Private)
Private=as.factor(Private)
plot(Outstate, Private)
private = as.factor(Private)
plot(Outstate, private)
plot(Outstate,as.factor(Private))
plot(as.factor(Private),Outstate)
plot(as.factor(Private),Outstate)
pdf("boxplots.pdf")
Elite = rep("No",nrow(as.factor(Private)))
Elite = rep("No",nrow(College))
Elite[College$Top10perc > 50] = "Yes"
Elite=as.factor(Elite)
College=data.frame(College,Elite)
summary(College)
plot(Elite,Outstate)
attach(College)
plot(Elite, College$Outstate)
plot(as.factor(Private),Outstate)
def.off()
dev.off()
dev.on()
plot(Elite,Outstate)
pdf("boxplots-elite.pdf")
dev.off()
dev.on()
par(mfrow=c(2,2))
hist(Private)
hist(Apps)
hist(Accept)
hist(Enroll)
hist(Grad.Rate)
pdf("histograms.pdf")
dev.off()
dev.on()
pairs()
pairs(College)
pdf("Pair-wise-plots.pdf")
plot(as.factor(Private),Expend)
dev.off()
plot(as.factor(Private),Expend)
save.history("problem8-RHistory")
savehistory("problem8-RHistory")
